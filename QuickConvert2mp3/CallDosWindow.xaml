<Window x:Class="QuickConvert2mp3.CallDosWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickConvert2mp3"
        mc:Ignorable="d"
        Title="Processing..." Height="40" Width="184" Visibility="Visible" Focusable="False" Background="{x:Null}" Foreground="{x:Null}" AllowsTransparency="True" WindowStyle="None" Opacity="0.5" MouseLeftButtonDown="AllowDrag" MouseRightButtonDown="CancelCheck">
    <Grid Margin="5,5,5,5">
        <Grid Background="#FFDCDCDC">
            <Grid.Effect>
                <BlurEffect Radius="9"/>
            </Grid.Effect>
        </Grid>
        <TextBox x:Name="textBoxShowRet" HorizontalAlignment="Left" Height="0" Margin="0,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="0" Background="{x:Null}" Visibility="Hidden"/>
        <Label x:Name="title" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Background="{x:Null}" Content="快捷转换 正在处理..." FontSize="18" BorderBrush="{x:Null}"/>
    </Grid>
</Window>
